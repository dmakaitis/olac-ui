{
  "name": "olac",
  "version": "0.1.0",
  "bin": {
    "olac": "bin/olac.js"
  },
  "scripts": {
    "lint": "eslint .",
    "clean": "rimraf -g 'dist' 'cdk.out' '*([lib|bin|test])/**/*.js' '*([lib|bin|test])/**/*.d.ts'",
    "compile": "tsc",
    "bundle": "node build.config.mjs",
    "build": "pnpm clean && pnpm compile && pnpm bundle",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "deploy-dev": "cdk deploy Dev/\\*",
    "deploy-test": "cdk deploy Test/\\*",
    "deploy-prod": "cdk deploy Prod/\\*"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.830.0",
    "@aws-sdk/client-lambda": "^3.830.0",
    "@aws-sdk/lib-dynamodb": "^3.830.0",
    "@types/aws-lambda": "^8.10.150",
    "aws-cdk-lib": "^2.201.0",
    "aws-jwt-verify": "^5.1.0",
    "axios": "^1.10.0",
    "constructs": "^10.0.0",
    "json-2-csv": "^5.5.9",
    "quasar": "workspace:*",
    "source-map-support": "^0.5.21",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@smithy/util-stream": "^4.2.2",
    "@types/jest": "^29.5.1",
    "@types/node": "^20.1.7",
    "aws-cdk": "^2.87.0",
    "aws-sdk": "^2.1414.0",
    "aws-sdk-client-mock": "^4.1.0",
    "aws-sdk-client-mock-jest": "^4.1.0",
    "esbuild": "^0.25.5",
    "jest": "^29.5.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "~5.1.3"
  }
}
